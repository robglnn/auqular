{
  "name": "auqular",
  "version": "1.0.0",
  "description": "Desktop video editor for Windows",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack --mode development && electron .",
    "build": "webpack --mode production",
    "dist": "webpack --mode production && electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "video",
    "editor",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "build": {
    "appId": "com.auqular.app",
